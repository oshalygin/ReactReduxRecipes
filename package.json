{
  "name": "react-redux",
  "version": "1.0.0",
  "description": "React with Redux and Express - Recipes",
  "scripts": {
    "prestart": "babel-node utilities/startingDisplayMessage.js",
    "start": "npm-run-all --parallel open:server lint:watch",
    "start-windows": "npm-run-all --parallel open-windows:server lint:watch",
    "open:server": "babel-watch ./server/application.js",
    "open-windows:server": "babel-node ./server/application.js",
    "lint": "node_modules/.bin/esw webpack.config.* client client/**/*.jsx server ./utilities",
    "lint:watch": "npm run lint -- --watch",
    "test:watch": "npm run tests -- --watch",
    "tests": "mocha --reporter min",
    "test": "babel-node ./node_modules/.bin/isparta cover --include 'server/**/*.js*' --include 'client/**/*.js*' _mocha",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node utilities/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node utilities/build.js",
    "postbuild": "babel-node utilities/dist.server.js"
  },
  "author": "Oleg Shalygin",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.13.1",
    "babel-polyfill": "6.8.0",
    "body-parser": "^1.15.2",
    "bootstrap": "3.3.6",
    "image-webpack-loader": "^2.0.0",
    "jquery": "2.2.3",
    "jsx-loader": "^0.13.2",
    "material-design-lite": "^1.1.3",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "babel-watch": "^2.0.2",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "coveralls": "^2.11.12",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.4",
    "jsdom": "8.5.0",
    "jsdom-global": "^2.0.0",
    "mocha": "2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "mock-require": "^1.3.0",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "15.0.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "wallaby-webpack": "0.0.22",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oshalygin/ReactReduxRecipes.git"
  }
}
